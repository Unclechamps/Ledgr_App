    <div class="row">
    <div class="col s12 m6 offset-m3">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text hoverable">
          <span class="card-title center">Sign Up</span>
          <div class="row">
            <form action='/signUp' method='post'class="col s12">
              <div class="row">
                <div class="input-field col s6">
                  <label for="first_name">First Name: </label>
                  <input class="textinput" type="text" name="first_name">
                </div>
                <div class="input-field col s6">
                  <label for="last_name">Last Name: </label>
                  <input class="textinput" type="text" name="last_name">
                </div>
              </div>
              <div class="row">
                <div class="input-field col s12">
                  <label for="email">Email: </label>
                  <input class="textinput" type="email" name="email">
                </div>
              </div>
              <div class="row">
                <div class="input-field col s12">
                  <label for="address">Address: </label>
                  <input class="textinput" type="text" name="address">
                </div>
              </div>
              <div class="row">
                <div class="input-field col s6">
                  <label for="username">Username: </label>
                  <input class="textinput" type="text" name="username">
                </div>

                <div class="input-field col s6">
                  <label for="password">Password: </label>
                  <input class="textinput" type="text" name="password">
                  <input class="submitBtn offset-m3" type="submit" />
                </div>
              </div>
              </div>
            </form>
          </div>
        </div>
        </div>
      </div>
    </div>

  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
