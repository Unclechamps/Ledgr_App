      <div class="card white">
        <div class="card-content white-text">
          <span class="card-title center black-text">Sign Up</span>
          <div class="row">
            <form action='/signUp' method='post'class="col s12">
              <div class="row">
                <div class="input-field col s6">
                  <label for="first_name">First Name </label>
                  <input class="textinput" type="text" name="first_name">
                </div>
                <div class="input-field col s6">
                  <label for="last_name">Last Name </label>
                  <input class="textinput" type="text" name="last_name">
                </div>
              </div>
              <div class="row">
                <div class="input-field col s12">
                  <label for="email">Email </label>
                  <input class="textinput" type="email" name="email">
                </div>
              </div>
              <div class="row">
                <div class="input-field col s12">
                  <label for="address">Address </label>
                  <input class="textinput" type="text" name="address">
                </div>
              </div>
              <div class="row">
                <div class="input-field col s6">
                  <label for="username">Username </label>
                  <input class="textinput" type="text" name="username">
                </div>

                <div class="input-field col s6">
                  <label for="password">Password </label>
                  <input class="textinput" type="password" name="password">
                </div>
                <button class="btn-large waves-effect waves-light red accent-2" type="submit" name="action">
                  Submit
                  <i class="material-icons right">send</i>
                </button>
              </div>
              </div>
            </form>
          </div>
        </div>
        </div>
